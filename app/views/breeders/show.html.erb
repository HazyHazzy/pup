<h1><%= @breeder.name %></h1>

<h2>About Us</h2>
<%= @breeder.about_us %>

<h3>Parents</h3>
<% @breeder.parents.reverse_each do |parent|%>
  <h2><%= link_to parent.name, breeder_parent_path(@breeder.id, parent.id) %></h2>
<% end %>

<h3>Litters</h3>
<% @breeder.litters.reverse_each do |litter|%>
  <h2><%= link_to litter.name, breeder_litter_path(@breeder.id, litter.id) %></h2>
<% end %>



