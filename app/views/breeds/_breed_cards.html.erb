<div class="breed-cards">
  <% @breeds.reverse_each do |breed|%>
    <div class="breed-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= image_path "Breed_bulldog.jpg" %>')">
      <%= link_to breed.name, breed_path(breed.id), class: "breed-name" %>
    </div>
  <% end %>
</div>
