<% if @breeder != [] %>
  <h1><%= @breeder[0].name %></h1>

  <h2>About Us</h2>
  <%= @breeder[0].about_us %>

  <h3>Parents</h3>
  <% @breeder[0].parents.reverse_each do |parent|%>
    <h2><%= link_to parent.name, breeder_parent_path(@breeder[0].id, parent.id) %></h2>
  <% end %>

  <h3>Litters</h3>
  <% @breeder[0].litters.reverse_each do |litter|%>
    <h2><%= link_to litter.name, breeder_litter_path(@breeder[0].id, litter.id) %></h2>
  <% end %>

  <h1>visitation Inquiry</h1>
  <% @my_visitations.reverse_each do |visitation| %>
    <h2>
      Request for visitation from <%= visitation.name %>
    </h2>
    <%= link_to "look into details for this visitation", edit_breeder_visitation_path(visitation.breeder_id, visitation.id), class: "btn btn-outline-dark" %>
  <% end %>
<% else %>
  <p>You don't have a brand page yet</p>
  <%= link_to "create my brand page", new_breeder_path, class: "btn btn-outline-dark" %>
<% end %>

